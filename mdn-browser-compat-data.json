        {
            "content_html": "<img src=\"https://avatars.githubusercontent.com/u/498917?u=a9115706328a9b324dae9b1f27cbb3bd507a9232&v=4\" width=\"64\" height=\"64\" alt=\"\"/><br/><p><a rel=\"nofollow\" href=\"https://developer.apple.com/documentation/ios-ipados-release-notes/ios-ipados-14_5-beta-release-notes\">https://developer.apple.com/documentation/ios-ipados-release-notes/ios-ipados-14_5-beta-release-notes</a></p>",
